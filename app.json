{
  "expo": {
    "name": "snack-custom-runtime",
    "slug": "snack-custom-runtime",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bycedric.snackcustomruntime"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.bycedric.snackcustomruntime"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "extra": {
      "cloudEnv": "production"
    }
  }
}
